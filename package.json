{
  "name": "uiuxu",
  "version": "1.0.0",
  "description": "Modern UI/UX Component Library with Vite Build System",
  "type": "module",
  "author": "asever77",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    },
    "./accordion": {
      "import": "./dist/accordion.esm.js",
      "require": "./dist/accordion.cjs"
    },
    "./button-selection": {
      "import": "./dist/buttonSelection.esm.js",
      "require": "./dist/buttonSelection.cjs"
    },
    "./chart-bubble": {
      "import": "./dist/chartBubble.esm.js",
      "require": "./dist/chartBubble.cjs"
    },
    "./countdown": {
      "import": "./dist/countdown.esm.js",
      "require": "./dist/countdown.cjs"
    },
    "./dialog": {
      "import": "./dist/dialog.esm.js",
      "require": "./dist/dialog.cjs"
    },
    "./drag": {
      "import": "./dist/drag.esm.js",
      "require": "./dist/drag.cjs"
    },
    "./dropdown": {
      "import": "./dist/dropdown.esm.js",
      "require": "./dist/dropdown.cjs"
    },
    "./form": {
      "import": "./dist/form.esm.js",
      "require": "./dist/form.cjs"
    },
    "./list-ia": {
      "import": "./dist/listIA.esm.js",
      "require": "./dist/listIA.cjs"
    },
    "./nav": {
      "import": "./dist/nav.esm.js",
      "require": "./dist/nav.cjs"
    },
    "./range-slider": {
      "import": "./dist/rangeSlider.esm.js",
      "require": "./dist/rangeSlider.cjs"
    },
    "./roulette": {
      "import": "./dist/roulette.esm.js",
      "require": "./dist/roulette.cjs"
    },
    "./scroll-event": {
      "import": "./dist/scrollEvent.esm.js",
      "require": "./dist/scrollEvent.cjs"
    },
    "./tab": {
      "import": "./dist/tab.esm.js",
      "require": "./dist/tab.cjs"
    },
    "./time-select": {
      "import": "./dist/timeSelect.esm.js",
      "require": "./dist/timeSelect.cjs"
    },
    "./toggle-controller": {
      "import": "./dist/toggleController.esm.js",
      "require": "./dist/toggleController.cjs"
    },
    "./tooltip": {
      "import": "./dist/tooltip.esm.js",
      "require": "./dist/tooltip.cjs"
    },
    "./wheel-picker": {
      "import": "./dist/wheelPicker.esm.js",
      "require": "./dist/wheelPicker.cjs"
    },
    "./utils/errors": {
      "import": "./dist/errors.esm.js",
      "require": "./dist/errors.cjs"
    },
    "./utils/logger": {
      "import": "./dist/logger.esm.js",
      "require": "./dist/logger.cjs"
    },
    "./utils": {
      "import": "./dist/utils.esm.js",
      "require": "./dist/utils.cjs"
    },
    "./styles": "./dist/uiuxu.css",
    "./dist/uiuxu.css": "./dist/uiuxu.css",
    "./uiuxu.css": "./dist/uiuxu.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asever77/UIUXU.git"
  },
  "bugs": {
    "url": "https://github.com/asever77/UIUXU/issues"
  },
  "homepage": "https://github.com/asever77/UIUXU#readme",
  "keywords": [
    "ui",
    "ux",
    "components",
    "javascript",
    "scss",
    "vite",
    "accordion",
    "dialog",
    "modal",
    "dropdown",
    "tooltip",
    "button-selection",
    "chart-bubble",
    "countdown",
    "drag-drop",
    "form",
    "list-ia",
    "navigation",
    "range-slider",
    "roulette",
    "scroll-event",
    "tab",
    "time-select",
    "toggle-controller",
    "wheel-picker",
    "ui-library",
    "component-library",
    "frontend"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.config.lib.js",
    "build:lite": "vite build --config vite.config.lite.js",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib",
    "scss:watch": "sass --watch src/assets/scss:src/assets/css",
    "scss:build": "sass src/assets/scss:src/assets/css --style=compressed",
    "lint": "eslint src/**/*.js",
    "clean": "rimraf dist dist-lite"
  },
  "devDependencies": {
    "vite": "^5.4.10",
    "sass": "^1.79.5",
    "terser": "^5.36.0",
    "eslint": "^9.13.0",
    "rimraf": "^6.0.1",
    "vite-plugin-html": "^3.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
